{
  "__inputs": [],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "9.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "9.0.0" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "9.0.0" },
    { "type": "panel", "id": "piechart", "name": "Pie chart", "version": "9.0.0" },
    { "type": "panel", "id": "bargauge", "name": "Bar gauge", "version": "9.0.0" },
    { "type": "panel", "id": "heatmap", "name": "Heatmap", "version": "9.0.0" }
  ],
  "annotations": { "list": [] },
  "description": "Production Master — investigation rate, success rate, and duration percentiles",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "asDropdown": false,
      "icon": "dashboard",
      "includeVars": true,
      "keepTime": true,
      "tags": ["production-master"],
      "targetBlank": false,
      "title": "Cost Tracking",
      "type": "link",
      "url": "/d/pm-cost-tracking"
    }
  ],
  "liveNow": false,
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "title": "Investigation Rate",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Total investigations started per second, by trigger source",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 15 },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 1 },
      "id": 2,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(pm_investigation_total{domain=~\"$domain\"}[$__rate_interval])) by (trigger_source)",
          "legendFormat": "{{ trigger_source }}",
          "refId": "A"
        }
      ],
      "title": "Investigation Rate by Trigger",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Total investigations started per second, by domain",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 15 },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 1 },
      "id": 3,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(pm_investigation_total{domain=~\"$domain\"}[$__rate_interval])) by (domain)",
          "legendFormat": "{{ domain }}",
          "refId": "A"
        }
      ],
      "title": "Investigation Rate by Domain",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Total investigations started (cumulative count)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 1 },
      "id": 4,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "area" },
      "targets": [
        {
          "expr": "sum(increase(pm_investigation_total{domain=~\"$domain\"}[$__range]))",
          "legendFormat": "Total",
          "refId": "A"
        }
      ],
      "title": "Total Investigations (Period)",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "id": 10,
      "title": "Success Rate",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Ratio of completed investigations to total finished (completed + failed)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.7 },
              { "color": "green", "value": 0.9 }
            ]
          },
          "min": 0,
          "max": 1,
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 10 },
      "id": 11,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "area" },
      "targets": [
        {
          "expr": "sum(rate(pm_investigation_verdict{verdict=\"completed\"}[1h])) / sum(rate(pm_investigation_verdict[1h]))",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ],
      "title": "Investigation Success Rate",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Verdict distribution over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 15, "stacking": { "mode": "normal" } },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 10 },
      "id": 12,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(pm_investigation_verdict[$__rate_interval])) by (verdict)",
          "legendFormat": "{{ verdict }}",
          "refId": "A"
        }
      ],
      "title": "Verdict Rate Over Time",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Distribution of investigation verdicts",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 10 },
      "id": 13,
      "options": { "legend": { "displayMode": "table", "placement": "right" }, "reduceOptions": { "calcs": ["lastNotNull"] }, "pieType": "donut" },
      "targets": [
        {
          "expr": "sum by (verdict) (pm_investigation_verdict)",
          "legendFormat": "{{ verdict }}",
          "refId": "A"
        }
      ],
      "title": "Verdict Distribution",
      "type": "piechart"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "id": 20,
      "title": "Duration Percentiles",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Investigation duration percentiles (p50, p90, p95, p99)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 19 },
      "id": 21,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(pm_investigation_duration_seconds_bucket{domain=~\"$domain\"}[$__rate_interval])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.90, sum(rate(pm_investigation_duration_seconds_bucket{domain=~\"$domain\"}[$__rate_interval])) by (le))",
          "legendFormat": "p90",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(pm_investigation_duration_seconds_bucket{domain=~\"$domain\"}[$__rate_interval])) by (le))",
          "legendFormat": "p95",
          "refId": "C"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(pm_investigation_duration_seconds_bucket{domain=~\"$domain\"}[$__rate_interval])) by (le))",
          "legendFormat": "p99",
          "refId": "D"
        }
      ],
      "title": "Investigation Duration Percentiles",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Investigation duration heatmap by domain",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "scheme", "schemeName": "Oranges" },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 19 },
      "id": 22,
      "options": { "calculate": false, "yAxis": { "unit": "s" } },
      "targets": [
        {
          "expr": "sum(increase(pm_investigation_duration_seconds_bucket{domain=~\"$domain\"}[$__rate_interval])) by (le)",
          "legendFormat": "{{ le }}",
          "format": "heatmap",
          "refId": "A"
        }
      ],
      "title": "Investigation Duration Heatmap",
      "type": "heatmap"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Duration percentiles by domain",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 27 },
      "id": 23,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(pm_investigation_duration_seconds_bucket[$__rate_interval])) by (domain, le))",
          "legendFormat": "{{ domain }} p95",
          "refId": "A"
        }
      ],
      "title": "p95 Duration by Domain",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 35 },
      "id": 30,
      "title": "Agents & Hypothesis Loop",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Agent invocation rate by agent name",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 15 },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 36 },
      "id": 31,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(pm_agent_invocation_total{domain=~\"$domain\"}[$__rate_interval])) by (agent_name)",
          "legendFormat": "{{ agent_name }}",
          "refId": "A"
        }
      ],
      "title": "Agent Invocation Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Agent duration percentiles (p50/p95) by agent name",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 36 },
      "id": 32,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(pm_agent_invocation_duration_seconds_bucket[$__rate_interval])) by (agent_name, le))",
          "legendFormat": "{{ agent_name }} p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(pm_agent_invocation_duration_seconds_bucket[$__rate_interval])) by (agent_name, le))",
          "legendFormat": "{{ agent_name }} p95",
          "refId": "B"
        }
      ],
      "title": "Agent Duration by Agent",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Hypothesis iterations per investigation (p50/p95/p99)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 15 },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 36 },
      "id": 33,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(pm_investigation_hypothesis_iterations_bucket{domain=~\"$domain\"}[$__rate_interval])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(pm_investigation_hypothesis_iterations_bucket{domain=~\"$domain\"}[$__rate_interval])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(pm_investigation_hypothesis_iterations_bucket{domain=~\"$domain\"}[$__rate_interval])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "title": "Hypothesis Iterations",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 44 },
      "id": 40,
      "title": "MCP Tool Calls",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "MCP tool call rate by server and status",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 15 },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 45 },
      "id": 41,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(pm_mcp_tool_call_total[$__rate_interval])) by (server, status)",
          "legendFormat": "{{ server }} ({{ status }})",
          "refId": "A"
        }
      ],
      "title": "MCP Tool Call Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "MCP tool call duration p50/p95 by server",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 45 },
      "id": 42,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(pm_mcp_tool_call_duration_seconds_bucket[$__rate_interval])) by (server, le))",
          "legendFormat": "{{ server }} p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(pm_mcp_tool_call_duration_seconds_bucket[$__rate_interval])) by (server, le))",
          "legendFormat": "{{ server }} p95",
          "refId": "B"
        }
      ],
      "title": "MCP Tool Call Duration",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "MCP error ratio per server (0 = healthy, 1 = all calls failing)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "red", "value": 0.5 }
            ]
          },
          "min": 0,
          "max": 1,
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 45 },
      "id": 43,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "horizontal", "displayMode": "gradient" },
      "targets": [
        {
          "expr": "sum(rate(pm_mcp_tool_call_total{status=\"error\"}[5m])) by (server) / sum(rate(pm_mcp_tool_call_total[5m])) by (server)",
          "legendFormat": "{{ server }}",
          "refId": "A"
        }
      ],
      "title": "MCP Server Error Ratio",
      "type": "bargauge"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["production-master", "observability", "investigations"],
  "templating": {
    "list": [
      {
        "current": {},
        "hide": 0,
        "includeAll": false,
        "name": "DS_PROMETHEUS",
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      },
      {
        "allValue": ".*",
        "current": { "selected": true, "text": "All", "value": "$__all" },
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "definition": "label_values(pm_investigation_total, domain)",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "name": "domain",
        "query": "label_values(pm_investigation_total, domain)",
        "refresh": 2,
        "sort": 1,
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Production Master — Investigation Overview",
  "uid": "pm-investigation-overview",
  "version": 1
}
