{
  "__inputs": [],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "9.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "9.0.0" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "9.0.0" },
    { "type": "panel", "id": "piechart", "name": "Pie chart", "version": "9.0.0" },
    { "type": "panel", "id": "bargauge", "name": "Bar gauge", "version": "9.0.0" },
    { "type": "panel", "id": "heatmap", "name": "Heatmap", "version": "9.0.0" }
  ],
  "annotations": { "list": [] },
  "description": "Production Master â€” investigation pipeline observability",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "title": "Overview",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Total investigations started over time",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 15 },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 1 },
      "id": 2,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(pm_investigation_total[$__rate_interval])) by (trigger)",
          "legendFormat": "{{ trigger }}",
          "refId": "A"
        }
      ],
      "title": "Investigation Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Number of investigations currently in progress",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 1 },
      "id": 3,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "area" },
      "targets": [
        {
          "expr": "sum(pm_investigation_duration_seconds_count) - sum(pm_investigation_verdict)",
          "legendFormat": "Active investigations",
          "refId": "A"
        }
      ],
      "title": "Active Investigations",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Distribution of investigation verdicts",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 1 },
      "id": 4,
      "options": { "legend": { "displayMode": "table", "placement": "right" }, "reduceOptions": { "calcs": ["lastNotNull"] }, "pieType": "donut" },
      "targets": [
        {
          "expr": "sum by (verdict) (pm_investigation_verdict)",
          "legendFormat": "{{ verdict }}",
          "refId": "A"
        }
      ],
      "title": "Verdict Distribution",
      "type": "piechart"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "id": 10,
      "title": "Performance",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Investigation duration histogram as heatmap",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "scheme", "schemeName": "Oranges" },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 10 },
      "id": 11,
      "options": { "calculate": false, "yAxis": { "unit": "s" } },
      "targets": [
        {
          "expr": "sum(increase(pm_investigation_duration_seconds_bucket[$__rate_interval])) by (le)",
          "legendFormat": "{{ le }}",
          "format": "heatmap",
          "refId": "A"
        }
      ],
      "title": "Investigation Duration",
      "type": "heatmap"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "p50/p95/p99 agent execution time by agent name",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 10 },
      "id": 12,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(pm_agent_duration_seconds_bucket[$__rate_interval])) by (agent, le))",
          "legendFormat": "{{ agent }} p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(pm_agent_duration_seconds_bucket[$__rate_interval])) by (agent, le))",
          "legendFormat": "{{ agent }} p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(pm_agent_duration_seconds_bucket[$__rate_interval])) by (agent, le))",
          "legendFormat": "{{ agent }} p99",
          "refId": "C"
        }
      ],
      "title": "Agent Duration by Agent",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Number of hypothesis iterations per investigation",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 15 },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 10 },
      "id": 13,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(pm_hypothesis_iterations_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(pm_hypothesis_iterations_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(pm_hypothesis_iterations_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "title": "Hypothesis Iterations",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "id": 20,
      "title": "MCP",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "MCP tool call latency by server (p50/p95)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 19 },
      "id": 21,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(pm_mcp_call_duration_seconds_bucket[$__rate_interval])) by (server, le))",
          "legendFormat": "{{ server }} p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(pm_mcp_call_duration_seconds_bucket[$__rate_interval])) by (server, le))",
          "legendFormat": "{{ server }} p95",
          "refId": "B"
        }
      ],
      "title": "MCP Call Latency by Server",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "MCP error rate per server",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 15 },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 19 },
      "id": 22,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(pm_mcp_call_errors_total[$__rate_interval])) by (server)",
          "legendFormat": "{{ server }}",
          "refId": "A"
        }
      ],
      "title": "MCP Error Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "MCP server health: error ratio per server (0 = healthy, 1 = all calls failing)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "red", "value": 0.5 }
            ]
          },
          "min": 0,
          "max": 1,
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 19 },
      "id": 23,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "horizontal", "displayMode": "gradient" },
      "targets": [
        {
          "expr": "sum(rate(pm_mcp_call_errors_total[5m])) by (server) / (sum(rate(pm_mcp_call_duration_seconds_count[5m])) by (server) + sum(rate(pm_mcp_call_errors_total[5m])) by (server))",
          "legendFormat": "{{ server }}",
          "refId": "A"
        }
      ],
      "title": "MCP Server Health",
      "type": "bargauge"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 },
      "id": 30,
      "title": "Cost",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Token usage rate by model (input + output)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 15, "stacking": { "mode": "normal" } },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 28 },
      "id": 31,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(pm_agent_tokens_total[$__rate_interval])) by (agent, direction)",
          "legendFormat": "{{ agent }} {{ direction }}",
          "refId": "A"
        }
      ],
      "title": "Token Usage by Model",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Cumulative and rate of LLM spend in USD",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "unit": "currencyUSD"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 28 },
      "id": 32,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(pm_llm_cost_dollars[$__rate_interval])) by (model)",
          "legendFormat": "{{ model }} rate/s",
          "refId": "A"
        },
        {
          "expr": "sum(pm_llm_cost_dollars) by (model)",
          "legendFormat": "{{ model }} cumulative",
          "refId": "B"
        }
      ],
      "title": "LLM Cost",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Current investigation queue depth",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 20 }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 28 },
      "id": 33,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "area" },
      "targets": [
        {
          "expr": "sum(pm_queue_depth) by (queue)",
          "legendFormat": "{{ queue }}",
          "refId": "A"
        }
      ],
      "title": "Queue Depth",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 36 },
      "id": 40,
      "title": "Workers",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Worker pool utilization (0-1)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.7 },
              { "color": "red", "value": 0.9 }
            ]
          },
          "min": 0,
          "max": 1,
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 37 },
      "id": 41,
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "horizontal", "displayMode": "gradient" },
      "targets": [
        {
          "expr": "pm_worker_utilization",
          "legendFormat": "{{ pool }}",
          "refId": "A"
        }
      ],
      "title": "Worker Utilization",
      "type": "bargauge"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "description": "Investigations completed per minute",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "lineWidth": 2, "fillOpacity": 15 },
          "unit": "opm"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 37 },
      "id": 42,
      "options": { "legend": { "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "expr": "sum(rate(pm_investigation_verdict[$__rate_interval])) * 60",
          "legendFormat": "Completed/min",
          "refId": "A"
        },
        {
          "expr": "sum(rate(pm_investigation_total[$__rate_interval])) * 60",
          "legendFormat": "Started/min",
          "refId": "B"
        }
      ],
      "title": "Job Processing Rate",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["production-master", "observability"],
  "templating": {
    "list": [
      {
        "current": {},
        "hide": 0,
        "includeAll": false,
        "name": "DS_PROMETHEUS",
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Production Master",
  "uid": "production-master",
  "version": 1
}
